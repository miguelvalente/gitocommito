{
  "name": "gitocommito",
  "displayName": "GitoCommito",
  "publisher": "mvalente",
  "description": "",
  "version": "0.0.5",
  "icon": "assets/logo.png",
  "engines": {
    "vscode": "^1.78.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/miguelvalente/gitocommito.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "GitoCommito",
      "properties": {
        "GitoCommito.OpenAIApiKey": {
          "type": "string",
          "default": "",
          "description": "Open AI Key"
        }
      }
    },
    "commands": [
      {
        "command": "extension.generateCommitMessage",
        "title": "GitoCommito: Generate Commit Message"
      },
      {
        "command": "extension.setOpenAIKey",
        "title": "GitoCommito: Set OpenAI Key"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@dqbd/tiktoken": "^1.0.7",
    "langchain": "^0.0.92",
    "openai": "^3.3.0"
  }
}
